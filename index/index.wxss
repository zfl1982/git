.cu-list.grid>.cu-item::after {
  content: " ";
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border-radius: inherit;
  transform: scale(0.5);
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border-right: none;
  border-bottom: none;
}
.cu-list.grid>.cu-item image {
  height: 80rpx;
  width: 80rpx;
  margin: auto;
}
